{
  "expo": {
    "name": "Quant Practice Pro",
    "slug": "quant-practice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "quant-practice",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#080d1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quantpractice.pro",
      "buildNumber": "1",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This data will be used to provide personalized study recommendations."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#080d1a"
      },
      "package": "com.quantpractice.pro",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "extra": {
      "eas": {
        "projectId": "YOUR_EAS_PROJECT_ID"
      }
    }
  }
}
